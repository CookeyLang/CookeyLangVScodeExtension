{
	"for": {
		"prefix": "for",
		"body": "for (var ${1} = ${2}; ${1} < ${3}; ${1}++) {\n\t${0}\n}",
		"description": "For loop"
	},

	"while": {
		"prefix": "while",
		"body": "while (${1:condition}) {\n\t${0}\n}",
		"description": "For loop"
	},

	"do while": {
		"prefix": "dowhile",
		"body": "do {\n\t${0}\n} while (${1:condition});",
		"description": "Do ... while ... loop"
	},

	"forrep": {
		"prefix": "forrep",
		"body": "forrep (${1:condition}) {\n\t${0}\n}",
		"description": "Forrep loop"
	},

	"class": {
		"prefix": "class",
		"body": "class ${1:Name} {\n\t${0}\n}",
		"description": "Class declaration"
	},

	"function": {
		"prefix": ["fn", "func", "function"],
		"body": "function ${1:name}(${2:arguments}) {\n\t${0}\n}",
		"description": "Function declaration"
	},

	"printLine": {
		"prefix": ["println", "printline"],
		"body": "printLine(${1});",
		"description": "Prints to the console."
	},

	"print": {
		"prefix": ["prnt", "print"],
		"body": "print(${1});",
		"description": "Prints without newline."
	},

	"if stmt": {
		"prefix": "if",
		"body": "if (${1:condition}) {\n\t${0}\n}",
		"description": "If statement"
	},

	"el if stmt": {
		"prefix": "elif",
		"body": "el if (${1:condition}) {\n\t${0}\n}",
		"description": "El if statement"
	},

	"if el stmt": {
		"prefix": "ifel",
		"body": "if (${1:condition}) {\n\t${3}\n} el if (${2:condition}) {\n\t${0}\n}",
		"description": "If ... el ... statement"
	},

	"lambda": {
		"prefix": "lambda",
		"body": "lambda (${1:arguments}): ${0}"
	}
}